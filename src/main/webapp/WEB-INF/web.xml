<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>


  <servlet>
    <servlet-name>AddNameZoo</servlet-name>
    <servlet-class>AddNameZoo</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>Select</servlet-name>
    <servlet-class>Select</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>Show</servlet-name>
    <servlet-class>Show</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>AddAnimal</servlet-name>
    <servlet-class>AddAnimal</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>AddNameZoo</servlet-name>
    <url-pattern>/addName</url-pattern>
  </servlet-mapping>


  <servlet-mapping>
    <servlet-name>Select</servlet-name>
    <url-pattern>/getTypes</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>Show</servlet-name>
    <url-pattern>/showZoo</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>AddAnimal</servlet-name>
    <url-pattern>/addAnimal</url-pattern>
  </servlet-mapping>
</web-app>
